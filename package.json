{
  "name": "cx-vue-cli",
  "description": "VUE 组件开发工具",
  "author": "wushain",
  "version": "0.1.0",
  "license": "MIT",
  "private": false,
  "main": "lib/cx-vue-cli.common.js",
  "scripts": {
    "lib": "vue-cli-service build --target lib --name cx-vue-cli --dest lib ./src/packages/index.ts",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "element-ui": "^2.4.5",
    "vue": "^2.6.10"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "vue",
    "element ui",
    "component"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/DzcxFE/cx-el-timeline.git"
  },
  "homepage": "https://github.com/DzcxFE/cx-el-timeline.git",
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-component": "^1.1.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-cli-plugin-element": "^1.0.1",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
